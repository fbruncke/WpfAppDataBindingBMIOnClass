<Window x:Class="WpfAppDataBindingBMIOnClass.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfAppDataBindingBMIOnClass"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="tbHeight" Text="{Binding Height}"/>
        <TextBox x:Name="tbWeight" Text="{Binding Weight}"/>
        <TextBox x:Name="tbResult" Text="{Binding Result}"/>
    </StackPanel>
</Window>
